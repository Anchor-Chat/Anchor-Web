    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Web Deploy",
            "program": "${workspaceFolder}/app/web_app/deploy.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Electron Main",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "program": "${workspaceFolder}/app/electron_app/index.js",
            "protocol": "inspector"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch build.js",
            "program": "${workspaceFolder}/app/web_app/build.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\app\\index.js"
        }
    ]
}